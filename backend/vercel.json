{
  "version": 3,
  "builds": [
    {
      "src": "backend/src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/src/(.*)",
      "dest": "/backend/src/$1.js"
    }
  ],
  "functions": {
    "backend/api/**/*.js": {
      "memory": 256,
      "maxDuration": 10
    }
  }
}
